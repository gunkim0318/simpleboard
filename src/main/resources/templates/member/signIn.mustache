{{>layout/header}} <!--</body>-->
<link rel="stylesheet" href="/css/member/signIn.css"/>
<div class="col-xs-12 col-sm-12 col-md-4 col-lg-4 text-center" style="float:none; margin: 0 auto;">
    <h1>로그인</h1>
    <form  action="/member/signIn" method="post">
        <input name="_csrf" type="hidden" value="{{_csrf.token}}" />
        <div class="input-group input-group-lg" style="margin-bottom: 10px;">
            <span class="input-group-addon" id="sizing-addon1"><span class="glyphicon glyphicon-envelope"></span></span>
            <input name="email" type="text" class="form-control" placeholder="Email">
        </div>
        <div class="input-group input-group-lg" style="margin-bottom: 10px;">
            <span class="input-group-addon" id="sizing-addon1"><span class="glyphicon glyphicon-eye-close"></span></span>
            <input name="password" type="password" class="form-control" placeholder="Password">
        </div>
        <div>
            <button id="signInBtn" class="btn btn-primary" style="width: 100%;">로그인</button>
            <hr/>
            <button type="button" id="signUpBtn" class="btn btn-success" style="width: 100%;">회원가입</button>
        </div>
    </form>
</div>
{{>layout/footer}} <!--</body>-->
<script src="/js/member/signIn.js"></script>
